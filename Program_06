import java.util.Scanner;
/*Method OverLoading is done by using same Method name but with different parameter, same is done here we have used 2 method sum() and sum(int,int) so that it is considered as 2 saperate method.*/

public class OverloadExample {
	
		public int sum(){
			int x=25,y=90;
			int z=x+y;
			return z;
			
		}
	//Method Overloading is Done here using Same SUM() and SUM(int,int)
		public int sum(int x,int y){
		
			int z=x+y;
			return z;
		}
	
	public static void main(String[] args) {
		int x=89,y=25;
		System.out.println("Method Overloading using SUM() Method\n"); 
		OverloadExample obj=new OverloadExample();
		obj.sum();
		System.out.println("\nSum of Number for SUM()= " +obj.sum());

		obj.sum(x,y);
		System.out.println("\nSum of Number for SUM(int ,int)= " +obj.sum(x,y));

	}

}

